{
  "name": "Config Override Development",
  "dockerFile": "Dockerfile",
  "context": "..",
  "runArgs": ["--publish", "8000:8000", "--publish", "8001:8001", "--publish", "8080:8080"],
  "remoteEnv": {
    "CONTAINER_NAME": "config-override"
  },
  "forwardPorts": [8000, 8001, 8080],
  "postStartCommand": "sudo /usr/bin/supervisord -c /etc/supervisor/supervisord.conf",
  "remoteUser": "vscode"
}
