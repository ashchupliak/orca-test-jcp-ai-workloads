{
  "name": "Config Override Test - Alternative",
  "image": "python:3.11-slim",
  "runArgs": ["-p", "8000:8000", "-p", "8001:8001", "-p", "8080:8080"],
  "postCreateCommand": "bash /workspaces/orca-test-jcp-ai-workloads/.devcontainer-config-override-alt/init.sh",
  "postStartCommand": "bash /workspaces/orca-test-jcp-ai-workloads/common/start-all.sh",
  "forwardPorts": [8000, 8001],
  "remoteEnv": {
    "CONFIG_SOURCE": "alternative-config",
    "CONFIG_TYPE": "path",
    "CONTAINER_NAME": "config-override-alternative",
    "TEST_MODE": "devcontainer-alternative",
    "PYTHONPATH": "/workspaces/orca-test-jcp-ai-workloads",
    "ALTERNATIVE_CONFIG": "true"
  }
}
