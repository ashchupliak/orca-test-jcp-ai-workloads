{
  "name": "Grazie Development",
  "dockerFile": "Dockerfile",
  "context": "..",
  "runArgs": ["-p", "8000:8000", "-p", "8001:8001", "-p", "8080:8080"],
  "remoteEnv": {
    "CONTAINER_NAME": "grazie"
  },
  "forwardPorts": [8000, 8001, 8080],
  "postStartCommand": "sudo /usr/bin/supervisord -c /etc/supervisor/supervisord.conf",
  "remoteUser": "vscode"
}
